{
    "name": "packages/delivery-infra",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Delivery\\DeliveryInfra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Delivery\\DeliveryInfra\\Tests\\": "tests/"
        }
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "Delivery\\DeliveryInfra\\DeliveryInfraServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
