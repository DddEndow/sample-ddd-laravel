{
    "name": "packages/delivery-domain",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/uid": "^6.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^6.23",
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Delivery\\Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Delivery\\Domain\\Tests\\": "tests/"
        }
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "Delivery\\Domain\\DeliveryDomainServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
